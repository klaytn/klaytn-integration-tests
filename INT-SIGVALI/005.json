{
  "tcID": "INT-SIGVALI-005",
  "tcName": "ValueTransfer.FailKey",
  "test": [
    {
      "tx": {
        "type": "LEGACY",
        "to": "random",
        "value": "0x90000000000000",
        "gas": 100000
      },
      "expected": {
        "status": true
      }
    },
    {
      "tx": {
        "type": "ACCOUNT_UPDATE",
        "from": "random",
        "gas": 100000,
        "accountKey": {
          "key": {},
          "keyType": 3
        },
        "privateKey": "random.privateKey"
      },
      "expected": {
        "status": true
      }
    },
    {
      "tx": {
        "type": "VALUE_TRANSFER",
        "from": "random",
        "privateKey": "random.privateKey",
        "to": "0x18fd72128ae95d0c72dc1301db27371cb22cd004",
        "value": "0x1",
        "gas": 100000
      },
      "expected": {
        "status": false,
        "errorString": "invalid transaction v, r, s values of the sender"
      }
    }
  ]
}
